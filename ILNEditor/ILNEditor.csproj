<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <Version>1.0.1</Version>
    <Copyright>Copyright Â© 2017-2020 Thilo Viereck</Copyright>
    <RepositoryUrl>https://github.com/profix898/ILNEditor</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Authors>Thilo Viereck</Authors>
    <Company />
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Properties editor and plot browser for ILNumerics (http://ilnumerics.net/) scene graphs and plot cubes</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <NeutralLanguage>en-US</NeutralLanguage>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="ILNumerics_deploy\ILNHelper.cs" Condition="$(EnableDefaultCompileItems) != 'true'" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="EMG.External.ILNumerics" Version="5.5.7504" />
  </ItemGroup>
  <ItemGroup>
    <None Include="ILNumerics_deploy\ILNImports1.targets" />
    <None Include="ILNumerics_deploy\ILNImports2.targets" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="ILNumerics_deploy\ilnumerics.lic" />
  </ItemGroup>
  <Import Project="ILNumerics_deploy\ILNImports1.targets" Condition="$(ILNLicCompile) != 'true'" />
  <Import Project="ILNumerics_deploy\ILNImports2.targets" Condition="$(ILNLicCompile) == 'true'" />
</Project>